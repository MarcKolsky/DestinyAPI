<Window x:Class="TCOB_Tracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TCOB_Tracker"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="932.67" Background="Black" WindowStartupLocation="CenterScreen" Topmost="True">
    <Grid>
        <TextBox x:Name="_result" HorizontalAlignment="Left" Height="750" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="905" Background="#FF9E9E9E" ScrollViewer.VerticalScrollBarVisibility="Visible" Visibility="Visible"/>
        <ListBox x:Name="_listbox" HorizontalAlignment="Left" Height="750" Margin="10,10,0,0" VerticalAlignment="Top" Width="905" Background="#FFACACAC" Visibility="Collapsed"/>
    </Grid>
</Window>
